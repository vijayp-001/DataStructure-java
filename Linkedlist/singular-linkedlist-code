
import java.util.*;
public class singularlinkedlist{
 Node head;
 class Node{
    int data;
    Node next;
} 
 
 public void insert(int data)
 {
    
    Node node = new Node();
    node.data=data;
    node.next=null;
    
    if(head==null)
    {
       head=node;
    }
    else{
       Node n=head;
       while(n.next!=null)
       {
          n=n.next;}
       n.next=node;
    }
 }
 
 public void print(){
  Node node =head;
    while(node.next!=null)
    {
       System.out.print(node.data +" ");
       node=node.next;
    }
    System.out.println(node.data);
 }

 public static void main(String args[]){
	 singularlinkedlist l = new singularlinkedlist();
   
    l.insert(7);
    l.insert(8);
    l.insert(9);
    l.print();
 }
}
